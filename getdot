#!/bin/bash
# this file will update itself when ran

echo "getdot  ::>>>>"
repo="dotfile"
url="https://github.com/suchdatums/$repo"
git clone --depth=1 $url
rm -rf ./$repo/.git
mv -f ./$repo/* .
rm -r ./$repo/

mv -f bash_aliases .bash_aliases

# give myself permission... will this work?
chmod +x getdot
go
echo "UPDATED TO VERSION 5.55"
